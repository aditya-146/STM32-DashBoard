/**
  * @brief GPIO Initialization Function
  * @param None
  * @retval None
  */
static void MX_GPIO_Init(void)
{
  GPIO_InitTypeDef GPIO_InitStruct = {0};
  /* USER CODE BEGIN MX_GPIO_Init_1 */

  /* USER CODE END MX_GPIO_Init_1 */

  /* GPIO Ports Clock Enable */
  __HAL_RCC_GPIOC_CLK_ENABLE();
  __HAL_RCC_GPIOH_CLK_ENABLE();
  __HAL_RCC_GPIOA_CLK_ENABLE();
  __HAL_RCC_GPIOB_CLK_ENABLE();

  /*Configure GPIO pin Output Level */
  HAL_GPIO_WritePin(GPIOA, GPIO_PIN_5|GPIO_PIN_9, GPIO_PIN_RESET);

  /*Configure GPIO pin : B1_Pin */
  GPIO_InitStruct.Pin = B1_Pin;
  GPIO_InitStruct.Mode = GPIO_MODE_IT_FALLING;
  GPIO_InitStruct.Pull = GPIO_NOPULL;
  HAL_GPIO_Init(B1_GPIO_Port, &GPIO_InitStruct);

  /*Configure GPIO pins : PA5 PA9 */
  GPIO_InitStruct.Pin = GPIO_PIN_5|GPIO_PIN_9;
  GPIO_InitStruct.Mode = GPIO_MODE_OUTPUT_PP;
  GPIO_InitStruct.Pull = GPIO_NOPULL;
  GPIO_InitStruct.Speed = GPIO_SPEED_FREQ_LOW;
  HAL_GPIO_Init(GPIOA, &GPIO_InitStruct);

  /* USER CODE BEGIN MX_GPIO_Init_2 */

  /* USER CODE END MX_GPIO_Init_2 */
}

/* USER CODE BEGIN 4 */

/* USER CODE END 4 */

/* USER CODE BEGIN Header_UltrasonicSensorTask */
/**
  * @brief  Function implementing the UltrasonicSenso thread.
  * @param  argument: Not used
  * @retval None
  */
/* USER CODE END Header_UltrasonicSensorTask */
void UltrasonicSensorTask(void *argument)
{
  /* USER CODE BEGIN 5 */
	uint32_t Difference = 0;
	uint8_t Distance = 0;
  /* Infinite loop */
  for(;;)
  {
	HCSR04_Read(&htim2);
	osMessageQueueGet(EchoQueueHandle, &Difference, NULL, osWaitForever);
	Distance = Difference * .034/2;
	printf("Distance: %u\n", Distance);
	osMessageQueuePut(UltrasonicQueueHandle, &Distance, 0, 0);
    osDelay(100);
  }
  /* USER CODE END 5 */
}

/* USER CODE BEGIN Header_WaterLevelTask */
/**
* @brief Function implementing the WaterLevelHandl thread.
* @param argument: Not used
* @retval None
*/
/* USER CODE END Header_WaterLevelTask */
void WaterLevelTask(void *argument)
{
  /* USER CODE BEGIN WaterLevelTask */
	uint32_t Sensor_Value = 0;
  /* Infinite loop */
  for(;;)
  {
	HAL_ADC_Start(&hadc1);
	HAL_ADC_PollForConversion(&hadc1, HAL_MAX_DELAY);

	Sensor_Value = HAL_ADC_GetValue(&hadc1);
	HAL_ADC_Stop(&hadc1);

	printf("Sensor Reading: %lu\n", Sensor_Value);
    osMessageQueuePut(WaterLevelQueueHandle, &Sensor_Value, 0, 0);
    osDelay(500);
  }
  /* USER CODE END WaterLevelTask */
}

/* USER CODE BEGIN Header_GyroTask */
/**
* @brief Function implementing the GyroHandle thread.
* @param argument: Not used
* @retval None
*/
/* USER CODE END Header_GyroTask */
void GyroTask(void *argument)
{
  /* USER CODE BEGIN GyroTask */
	  float ZTilt;
	  float Tilt_Offset;

	  HAL_StatusTypeDef ret = GY521_Connection();
	  if (ret == HAL_OK){
	  	  printf("The device is ready \n");
	  }else{
	  	  printf("U fucked gng \n");
	  	  Error_Handler();
	  }

	  GY521_Config();

	  Tilt_Offset = GY521_Calculate_Offset();
  /* Infinite loop */
  for(;;)
  {
	ZTilt = GY521_Calculate_Tilt(Tilt_Offset);
	printf("ZTilt = %.2f deg\n", ZTilt);
    osDelay(200);
  }
  /* USER CODE END GyroTask */
}

